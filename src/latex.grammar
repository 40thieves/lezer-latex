@top[name=LaTeX] { (text | command | Math)+ }

@skip { space }

command {
  BeginCommand |
  Command
}

Command { backslash text Options? Arguments? }

BeginCommand { backslash @specialize<text, "begin"> Options? Arguments }

Options { "[" text? "]" }

Arguments { "{" text? "}" }

Math { "$" (text | Command)? "$" }

@tokens {
  space { std.whitespace+ }

  backslash { "\\" }
  text { std.asciiLetter+ }
}
